<?xml version="1.0"?>
<odoo>

        <record id="estate_property_tag_view_form" model="ir.ui.view">
        <field name="name">estate.estate.property.tag.form</field>
        <field name="model">estate.estate.property.tag</field>
        <field name="arch" type="xml">
             <form string="Property Tag">
                 <sheet>
                     <group>
                        <field name="name"/>
                     </group>                   
                 </sheet>
             </form>
        </field>
    </record>

    
    <record id="estate_property_tag_view_tree" model="ir.ui.view">
        <field name="name">estate.estate.property.tag.tree</field>
        <field name="model">estate.estate.property.tag</field>
        <field name="arch" type="xml">
            <tree string="Properties tag">
                <field name="name"/>
            </tree>
        </field>
    </record>
    
    <record id="estate_property_tags_action" model="ir.actions.act_window">
        <field name="name">Property tag</field>
        <field name="res_model">estate.estate.property.tag</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>